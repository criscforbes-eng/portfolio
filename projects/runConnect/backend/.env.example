# Copy this file to .env and fill with real values (DO NOT commit .env)

# MongoDB connection string (username/password in the URI)
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.example.mongodb.net/runConnectDB?retryWrites=true&w=majority

# Server port (optional)
PORT=3000

# Frontend origin for CORS during development
FRONTEND_ORIGIN=http://localhost:8000

# If you use JWT or other secrets
JWT_SECRET=replace_with_a_secure_random_string